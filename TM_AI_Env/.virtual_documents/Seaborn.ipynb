import matplotlib.pyplot as plt
import pandas as pd
import seaborn as sns


sns.get_dataset_names()


penguins = sns.load_dataset("penguins")
penguins


print(penguins.to_string())


sns.relplot(x="body_mass_g", y="flipper_length_mm", data=penguins, kind="line")
plt.show()


sns.scatterplot(x="body_mass_g", y="flipper_length_mm", data=penguins, hue="species")


sns.scatterplot(
    x="body_mass_g", y="flipper_length_mm", data=penguins, hue="species", style="island"
)


sns.scatterplot(
    x="body_mass_g",
    y="flipper_length_mm",
    data=penguins,
    hue="species",
    size="sex",
    style="island",
)


sns.scatterplot(
    x="body_mass_g",
    y="flipper_length_mm",
    data=penguins,
    hue="species",
    size="sex",
    style="island",
    palette="rainbow",
)


tips = sns.load_dataset("tips")
sns.barplot(
    x="day",
    y="tip",
    hue="sex",
    data=tips,
)

plt.xlabel("Day")
plt.ylabel("Average Tip Amount")
plt.title("Average Tip amount by Day")


avg_tip_male_thursday = tips[(tips["day"] == "Thur") & (tips["sex"] == "Male")][
    "tip"
].mean()

print(f"Average tip amount given by men on Thursday: {avg_tip_male_thursday:.2f}")


avg_tip_female_thursday = tips[(tips["day"] == "Thur") & (tips["sex"] == "Female")][
    "tip"
].mean()

print(f"Average tip amount given by men on Thursday: {avg_tip_female_thursday:.2f}")


avg_tip_male_friday = tips[(tips["day"] == "Fri") & (tips["sex"] == "Male")][
    "tip"
].mean()

print(f"Average tip amount given by men on Thursday: {avg_tip_male_friday:.2f}")


